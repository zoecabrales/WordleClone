{
  "expo": {
    "name": "Word Master",
    "slug": "wordleClone",
    "version": "1.0.2",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "icon": "./assets/icon.png",
    "splash": {
      "backgroundColor": "#121213",
      "resizeMode": "native"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.wordmaster.app",
      "splash": {
        "backgroundColor": "#121213",
        "resizeMode": "native"
      }
    },
    "android": {
      "package": "com.wordmaster.app",
      "versionCode": 4,
      "permissions": [],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#121213"
      },
      "splash": {
        "backgroundColor": "#121213",
        "resizeMode": "native"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "9b5ec1b0-7468-4f6d-a6aa-401126f2f6a9"
      }
    },
    "owner": "jirehzoe"
  }
}
