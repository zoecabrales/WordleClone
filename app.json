{
  "expo": {
    "name": "Word Master",
    "slug": "wordleClone",
    "version": "1.0.2",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.wordmaster.app"
    },
    "android": {
      "package": "com.wordmaster.app",
      "versionCode": 3,
      "permissions": []
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "wordMaster"
      }
    }
  }
}
