{
  "expo": {
    "name": "Wordle Clone",
    "slug": "wordleClone",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.wordleclone.app"
    },
    "android": {
      "package": "com.wordleclone.app",
      "versionCode": 1,
      "permissions": []
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "9b5ec1b0-7468-4f6d-a6aa-401126f2f6a9"
      }
    }
  }
}
